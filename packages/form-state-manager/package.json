{
  "name": "@data-driven-forms/form-state-manager",
  "version": "0.0.1-beta.0",
  "description": "Form state management library for data driven forms",
  "main": "index.js",
  "module": "esm/index.js",
  "author": "Martin Marosi",
  "typings": "index.d.ts",
  "repository": "git@github.com:data-driven-forms/react-forms.git",
  "keywords": [
    "react",
    "forms",
    "javascript"
  ],
  "devDependencies": {
    "@typescript-eslint/parser": "^4.18.0"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "start": "webpack-dev-server --env dev --config ./config/webpack.config.js --open --hot",
    "build": "yarn build:cjs && yarn build:esm && yarn build:packages",
    "build:cjs": "tsc --module commonjs --target es5 --outDir ./",
    "build:esm": "tsc --module es2015 --target es5 --outDir esm",
    "build:packages": "node ../../scripts/generate-packages.js"
  }
}
