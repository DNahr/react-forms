        "@material-ui/styles": "^4.10.0",
        "@mdx-js/loader": "^1.6.22",
        "@mdx-js/react": "^1.6.22",
        "@next/bundle-analyzer": "^11.1.2",
        "@next/mdx": "^11.1.2",
        "@stackblitz/sdk": "^1.5.2",
        "clsx": "^1.1.1",
        "codesandbox": "2.2.1",
